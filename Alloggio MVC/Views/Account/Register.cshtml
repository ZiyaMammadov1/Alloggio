
@{
    ViewData["Title"] = "Register";
}
<!-- Check header begin -->
<div class="outHeader">
    <div class="outHeader_checkout">
        Register
    </div>
</div>
<!-- Check header end -->
<!-- Register begin -->
<div class="register">
    <form action="">
        <div class="register_group">
            <div class="register_groupName">
                Fullname *
            </div>
            <div class="register_groupInput">
                <input type="text" class="register_input">
            </div>
        </div>

        <div class="register_group">
            <div class="register_groupName">
                Email *
            </div>
            <div class="register_groupInput">
                <input type="text" class="register_input">
            </div>
        </div>

        <div class="register_group">
            <div class="register_groupName">
                Phone *
            </div>
            <div class="register_groupInput">
                <input type="text" class="register_input">
            </div>
        </div>

        <div class="register_group">
            <div class="register_groupName">
                Password *
            </div>
            <div class="register_groupInput">
                <input type="password" class="register_input">
            </div>
        </div>

        <div class="register_group">
            <div class="register_groupName">
                Confirm Password *
            </div>
            <div class="register_groupInput">
                <input type="password" class="register_input">
            </div>
        </div>

        <div class="register_group">
            <div class="register_button">
                <a href="#" class="register_link">Register</a>
            </div>
        </div>
    </form>

</div>
<!-- Register end -->